openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout private_key.pem -out certificate.pem -config <(cat <<EOF
[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C=BR
ST=Minas Gerais
L=Matipo
O=Bushido
OU=Developmente
CN=ec2-18-188-125-150.us-east-2.compute.amazonaws.com

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = ec2-18-188-125-150.us-east-2.compute.amazonaws.com
EOF
)
